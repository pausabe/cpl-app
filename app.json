{
  "expo": {
    "name": "CPL",
    "description": "This application helps up to pray with biblical texts that the Lord Jesus himself used to pray. Currently it is only available in Catalan.",
    "slug": "CPLApp",
    "version": "5.78.0",
    "orientation": "portrait",
    "icon": "./src/Assets/icon/icon.png",
    "githubUrl": "https://github.com/pausabe/cpl-app",
    "primaryColor": "#ffffff",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./src/Assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#006064"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "cpl.cpl",
      "buildNumber": "78",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "cpl.cpl",
      "versionCode": 78,
      "userInterfaceStyle": "automatic",
      "icon": "./src/Assets/icon/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/Assets/icon/androidForeground.png",
        "backgroundImage": "./src/Assets/icon/androidBackground.png"
      },
      "permissions":  []
    }
  }
}
